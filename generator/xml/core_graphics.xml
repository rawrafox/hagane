<framework module="core_graphics" no-prelude>
  <alias name="CGFloat" type="f64" />

  <structure name="CGPoint">
    <field name="x" type="CGFloat" />
    <field name="y" type="CGFloat" />
  </structure>

  <structure name="CGSize">
    <field name="width" type="CGFloat" />
    <field name="height" type="CGFloat" />
  </structure>

  <structure name="CGRect">
    <field name="origin" type="CGPoint" />
    <field name="size" type="CGSize" />
  </structure>
</framework>
